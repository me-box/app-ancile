{
  "name": "driver-ancile",
  "version": "1.0.0",
  "description": "A Databox driver to support Cornell Tech's Ancile Project",
  "config": {
    "registry": "localhost:5000"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha",
    "start": "node ./src/main.js",
    "build": "docker build -t databox/driver-motruelayernzo .",
    "deploy": "docker tag databox/driver-ancile $npm_package_config_registry/driver-ancile && docker push $npm_package_config_registry/driver-ancile",
    "build-arm": "docker build -f Dockerfile-arm -t databox/driver-ancile-arm .",
    "deploy-arm": "docker tag databox/driver-ancile-arm $npm_package_config_registry/driver-ancile-arm && docker push $npm_package_config_registry/driver-ancile-arm",
    "clean": "node ./node_modules/modclean/bin/modclean.js -p ./node_modules/ -r",
    "getshell": "docker exec -i -t driver-ancile /bin/bash",
    "kill": "docker kill driver-ancile",
    "attach": "docker attach driver-ancile",
    "shell": "docker run -i -t databox/driver-ancile /bin/bash",
    "local": "docker run -i -t databox/driver-ancile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minoskt/driver-ancile.git"
  },
  "keywords": [
    "Databox",
    "ancile",
    "stream"
  ],
  "author": "Kleomenis Katevas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minoskt/driver-ancile/issues"
  },
  "homepage": "https://github.com/minoskt/driver-ancile#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "modclean": "^2.1.2",
    "node-databox": "^0.10.6",
    "nonce-generator": "^1.0.2",
    "oauth": "^0.9.15",
    "pug": "^2.0.4",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "assert": "^1.5.0",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0"
  }
}
